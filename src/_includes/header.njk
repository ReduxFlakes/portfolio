<header class="sky-brand-header full-width">
<section class="brand-header-top">
    <h1>reduxflakes</h1>
    <section>
        <h2 class="subheading">connect</h2>
        <svg style="margin: auto;" xmlns="http://www.w3.org/2000/svg" width="1.2em" height="1.2em" viewBox="0 0 24 24"><rect width="6" height="14" x="1" y="4" fill="currentColor"><animate id="svgSpinnersBarsFade0" fill="freeze" attributeName="opacity" begin="0;svgSpinnersBarsFade1.end-0.3s" dur="0.9s" values="1;0.2"/></rect><rect width="6" height="14" x="9" y="4" fill="currentColor" opacity="0.4"><animate fill="freeze" attributeName="opacity" begin="svgSpinnersBarsFade0.begin+0.18s" dur="0.9s" values="1;0.2"/></rect><rect width="6" height="14" x="17" y="4" fill="currentColor" opacity="0.3"><animate id="svgSpinnersBarsFade1" fill="freeze" attributeName="opacity" begin="svgSpinnersBarsFade0.begin+0.36s" dur="0.9s" values="1;0.2"/></rect></svg>
    </section>
</section>
<nav class="sky-complex-navbar">
{% for item in nav.navbar %}
    <details class="navbar-menu">
        <summary class="navbar-button">
        <img src="/static/icons/{{item.icon}}.png" alt="{{item.title}} icon" style="width: 1.25rem;height: 1.25rem;">
        {{item.title}}
        </summary>
        <section class="menu-content">
        {% for meta in item.menu %}
        <small>{{meta.description}}</small>
        <nav class="menu-nav">
        {% for links in meta.links %}
            <a class="menu-nav__link" href="{{links.url}}"><img src="/static/icons/{{links.icon}}.png" alt="{{links.title}} icon" width="24px" height="24px">{{links.title}}
            <span class="context">{{links.context}}</span>
            </a>
        {% endfor %}
    </nav>
        {% endfor %}
        </section>
    </details>
{% endfor %}
</nav>
</header>